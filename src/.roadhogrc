{
  "entry": "src/index.js",
  "publicPath": "./",
  "outputPath": "../www",
  "autoprefixer": {
    "browsers": [
      "iOS >= 8", "Android >= 4"
    ]
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  },
  "extraBabelPlugins": [
      ["import", { "libraryName": "antd-mobile", "style": "css" }]
    ]
}
