{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["iOS >= 8", "Android >= 4"]
      },
      "uglify": true,
      "loose": true,
      "useBuiltIns": true
    }],
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    "add-module-exports",
    "transform-runtime",
    ["import", [{"libraryName": "antd-mobile", "style": "css"}, {"libraryName": "react-router", "camel2DashComponentName": false}]]
  ],
  "env": {
    "development": {
      "plugins": ["dva-hmr"]
     }
   }
}
